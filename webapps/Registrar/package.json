{
  "name": "Registrar",
  "version": "1.0.0",
  "author": "LOF/MK",
  "license": "LOF",
  "description": "Task manager",
  "scripts": {
    "postinstall": "npm run typings install",
    "typings": "typings",
    "clean": "rm -rf js",
    "build": "npm run clean && webpack --config config/webpack.config.js",
    "build:w": "npm run clean && webpack -w --config config/webpack.config.js",
    "build:p": "npm run clean && webpack --config config/webpack.prod.config.js --progress --profile --colors --display-error-details --display-cached"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "^2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",
    "moment": "2.14.1",
    "ng2-translate": "^2.4.3",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "css-loader": "^0.24.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.1",
    "raw-loader": "^0.5.1",
    "to-string-loader": "^1.1.4",
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.2",
    "typings": "^1.3.2",
    "webpack": "^2.1.0-beta.22"
  }
}
